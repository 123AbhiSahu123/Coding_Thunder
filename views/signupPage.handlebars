<div class="container my-5">
  <form id="SignUpForm">
    <h2 class="text-center bg-primary text-white">SignUP</h2>
    <div class="mb-3">
      <label for="name" class="form-label">Name</label>
      <input type="name" class="form-control" id="name" aria-describedby="nameHelp">
    </div>
    <div class="mb-3">
      <label for="email" class="form-label">Email address</label>
      <input type="email" class="form-control" id="email" aria-describedby="emailHelp">
      <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
    </div>
    <div class="mb-3">
      <label for="password" class="form-label">Password</label>
      <input type="password" class="form-control" id="password">
    </div>
    <div class="mb-3">
      <label for="phone" class="form-label">Phone</label>
      <input type="phone" class="form-control" id="phone" aria-describedby="phoneHelp">
    </div>
    <div class="mb-3">
      <label class="form-label" for="course">Course Name</label>
      <input type="course" class="form-control" id="course" aria-describedby="courseHelp">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
  </form>
</div>

<script src="/js/signup.js"></script>

















